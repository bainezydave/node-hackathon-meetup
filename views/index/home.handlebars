<div class="container">
  <div class="member-home-bounds">
    <div class="columns">
      <div class="column">
        <div class="attend-events">
          <div class="hero-banner-header">
            <h1>The real world is calling</h1>
          </div>
          <div class="attend-events-p">
            <p>Attend local events to meet people, try something <br> new, or do more of what you love.</p>
            <a href="/events"><button style="margin-top:15px;" class="button is-danger is-medium">Browse Events</button></a>
          </div>
        </div>
      </div>
      <div class="column">
        <img class="hero-img" style="margin-right: 25px;" src="https://learn-a-skill.dev.meetuphq.io/static/media/KnittingAroundTown.655f6f4b.jpg"></img>
      </div>
    </div>
  </div>
  <div class="events-syd">
    <div class="e-syd-header" style="margin-top: 50px;">
      <h1>Here are some events in Sydney.</h1>
    </div>
  </div>
  <div class="card-group">
    <div class="columns is-3">
      {{#each events}}
      <a class="card-link" href="/events/show/{{this._id}}">
        <div class="column">
          <div class="e-syd-cards">
            <div class="card-image">
                <img src="{{this.photo}}" alt="{{this.name}}">
            </div>
            <div style="overflow: hidden;" class="card-content">
              <h1 style="font-size:11pt;color:#2e3e48;font-family:'Rubik',
              sans-serif;">{{this.name}}</h1>
              <p>{{this.details}}</p>
              <p>{{this.time}}</p>
            </div>
          </div>
        </div>
        </a>
      {{/each}}
    </div>
  </div>
</div>
